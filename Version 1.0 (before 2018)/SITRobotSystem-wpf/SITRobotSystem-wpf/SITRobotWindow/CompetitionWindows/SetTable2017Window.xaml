<Window x:Class="SITRobotSystem_wpf.SITRobotWindow.CompetitionWindows.SetTable2017Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SITRobotSystem_wpf.SITRobotWindow.CompetitionWindows"
        mc:Ignorable="d"
        Title="SetTableWindow2017" Height="300" Width="300">
    <Grid>
        <Button x:Name="start" Content="Button" HorizontalAlignment="Left" Margin="105,227,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.5,0.5" Click="start_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.44"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>

    </Grid>
</Window>
